{
    "name": "brotli",
    "executable_path": "brotli",
    "input_file_param":"{input_file}",
    "output_file_param":"{output_file}",
    "compression_params": ["-k", "@input_file_param","-o","@output_file_param"],
    "decompression_params": ["-d", "-k","@input_file_param","-o","@output_file_param"],
    "extension": "br"
}