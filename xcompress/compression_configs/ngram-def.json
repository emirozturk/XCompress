{
    "name": "ngram-compressor",
    "executable_path": "python3",
    "input_file_param": "{input_file}",
    "output_file_param": "{output_file}",
    "compression_params": ["ngramcompress.py", "@input_file_param", "-c", "-n", "3", "@output_file_param"],
    "decompression_params": ["ngramcompress.py", "@input_file_param", "-d", "-n", "3", "@output_file_param"],
    "extension": "ngram"
}
